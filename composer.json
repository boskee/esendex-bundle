{
    "name": "boskee/esendex-bundle",
    "type": "symfony-bundle",
    "description": "Boskee Esendex Bundle for Symfony",
    "keywords": ["boskee", "esendex", "sms", "symfony", "bundle"],
    "homepage": "https://github.com/boskee/esendex-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Wojciech Zylinski",
            "email": "voitek@boskee.co.uk",
            "homepage": "http://boskee.co.uk/"
        }
    ],
    "support": {
        "email": "voitek@boskee.co.uk",
        "issues": "https://github.com/boskee/esendex-bundle/issues",
        "source": "https://github.com/boskee/esendex-bundle"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "esendex/sdk": "dev-master"
    },
    "autoload": {
        "psr-0": { "Boskee\\EsendexBundle": "" }
    },
    "target-dir": "Boskee/EsendexBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
